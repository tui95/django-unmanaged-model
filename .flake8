[flake8]
max-line-length = 120
extend-ignore =
    E203
    # flake8-annotations
    ANN002,
    ANN003,
    ANN101,
    ANN102,
per-file-ignores =
    manage.py:INP001
    django_unmanaged_model/settings/*.py:F401,F403
# flake8-tidy-imports
banned-modules =
  django_unmanaged_model.settings = Use `from django.conf import settings` instead
